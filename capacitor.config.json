{
  "appId": "com.lapaas.mindset",
  "appName": "Lapaas Mindset",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#ea580c",
      "showSpinner": false
    },
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon",
      "iconColor": "#ea580c",
      "sound": "default"
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#ea580c"
    }
  }
}